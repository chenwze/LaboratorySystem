<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.gdufe.laboratorysystem.dao.CodeDao">
    <insert id="addCode">
        INSERT INTO `laboratory_system`.`verification_code`(`id`, `code`, `end_time`, `start_time`, `email`)
        VALUES (#{id}, #{code}, #{endTime}, #{startTime}, #{email})
    </insert>

</mapper>